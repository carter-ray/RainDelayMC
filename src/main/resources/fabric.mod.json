	"authors": [
		"Carter Ray"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/carter-ray/RainDelayMC"
	},
	"license": "CC0-1.0",
	"icon": "assets/raindelay/icon.png",
	"environment": "*",
	"entrypoints": {
